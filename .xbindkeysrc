# Mute volumes
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

# Increase volume
"pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume

# Increase backlight
"light -A 5"
   XF86MonBrightnessUp

# Decrease backlight
"light -U 5"
   XF86MonBrightnessDown

# Screenshot
"flameshot full --path ~/screenshots"
    m:0x0 + c:107
    Print
